{
    "name": "moo/mooflashcard",
    "description": "Moo FlashCard Sandbox",
    "type": "project",
    "keywords": ["flash card", "Symfony2", "knowledgebase"],
    "homepage": "https://github.com/satrun77/MooFlashCardBundle-Example",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed",
            "email": "mohamed.alsharaf@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "": "src/", "SymfonyStandard\\": "app/SymfonyStandard/" }
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "~2.7",
        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "~1.5",
        "twig/extensions": "~1.2",
        "symfony/assetic-bundle": "~2.6",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.7",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.5",
        "incenteev/composer-parameter-handler": "~2.1",
        "doctrine/migrations": "~1.0",
        "doctrine/doctrine-migrations-bundle": "~1.0",
        "doctrine/data-fixtures" : "~1.1",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "sonata-project/core-bundle": "~2.3",
        "sonata-project/admin-bundle": "~2.3",
        "sonata-project/doctrine-orm-admin-bundle": "~2.3",
        "sonata-project/user-bundle": "~2.2",
        "sonata-project/easy-extends-bundle": "~2.1",
        "moo/flashcard-bundle": "dev-master",
        "moo/flashcardadmin-bundle": "~1.0"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    }
}

